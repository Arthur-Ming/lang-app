{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": [
      "vite-plugin-svgr/client"
    ],
    "baseUrl": "./",
    "paths": {
      "@App/*": [
        "src/App/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@/*": [
        "src/*"
      ],
      "@Header/*": [
        "src/components/Header/*"
      ],
      "@Footer/*": [
        "src/components/Footer/*"
      ],
      "@Textbook/*": [
        "src/pages/Textbook/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@action-types/*": [
        "src/redux/action-types/*"
      ],
      "@actions/*": [
        "src/redux/actions/*"
      ],
      "@reducer/*": [
        "src/redux/reducer/*"
      ],
      "@api/*": [
        "src/redux/api/*"
      ],
      "@selectors/*": [
        "src/redux/selectors/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@Loader/*": [
        "src/components/Loader/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@Modal/*": [
        "src/components/Modal/*"
      ],
      "@AudioButton/*": [
        "src/components/AudioButton/*"
      ],
      "@Games/*": [
        "src/pages/Games/*"
      ],
      "@PageRange/*": [
        "src/components/PageRange/*"
      ],
      "@GroupItem/*": [
        "src/components/GroupItem/*"
      ],
      "@GroupPicker/*": [
        "src/components/GroupPicker/*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}